{
  "manifest_version": 3,
  "name": "dbt Semantic Layer Explorer",
  "version": "1.0",
  "permissions": ["storage"],
  "web_accessible_resources": [
    {
      "resources": ["chart.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "host_permissions": ["http://localhost:7700/*", "https://semantic-layer.cloud.getdbt.com/api/graphql","https://cdn.jsdelivr.net/npm/chart.js","acme-demo.html"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "dbt-logo.png",
    "32": "dbt-logo.png",
    "48": "dbt-logo.png",
    "128": "dbt-logo.png"
  }
} 